<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="753dp"
    android:layout_marginTop="8dp"
    android:orientation="vertical"
    android:layout_height="36dp"
    android:background="#161718">
    <TextView
        android:layout_width="60dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:text="ID"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:id="@+id/tv_chong_id"
        />
    <TextView
        android:layout_width="134dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:text="充电指令时间"
        android:layout_marginLeft="3dp"
        android:layout_toRightOf="@id/tv_chong_id"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:id="@+id/tv_chong_time"
        />
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:text="对接结果"
        android:layout_marginLeft="3dp"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/tv_chong_time"
        android:id="@+id/tv_chong_result"
        />
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:text="开始充电时间"
        android:layout_toRightOf="@id/tv_chong_result"
        android:layout_marginLeft="3dp"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:id="@+id/tv_chong_start_time"
        />
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:text="开始充电电量"
        android:layout_marginLeft="3dp"
        android:layout_toRightOf="@id/tv_chong_start_time"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:id="@+id/tv_chong_start_num"
        />
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:text="结束充电电量"
        android:layout_marginLeft="3dp"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/tv_chong_start_num"
        android:id="@+id/tv_chong_end_num"
        />
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:text="结束充电时间"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/tv_chong_end_num"
        android:layout_marginLeft="3dp"
        android:id="@+id/tv_chong_end_time"
        />
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:text="充电耗时"
        android:layout_marginLeft="3dp"
        android:layout_toRightOf="@id/tv_chong_end_time"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:id="@+id/tv_chong_h_time"
        />

</RelativeLayout>