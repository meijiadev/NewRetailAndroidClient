                android:src="@mipmap/chongd_def"
                android:layout_centerVertical="true"
                android:id="@+id/iv_all_cd2"/>
        </RelativeLayout>
        <TextView
            android:layout_width="68dp"
            android:layout_height="24dp"
            android:text="设备名称："
            android:textSize="12sp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:layout_marginTop="48dp"
            android:id="@+id/tv_name2"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="24dp"
            android:layout_toRightOf="@id/tv_name2"
            android:layout_marginTop="48dp"
            android:textSize="12sp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:layout_marginLeft="12dp"
            android:id="@+id/tv_all_name2"/>
        <TextView
            android:layout_width="68dp"
            android:layout_height="24dp"
            android:text="当前电量："
            android:textSize="12sp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:layout_below="@id/tv_name2"
            android:layout_marginTop="18dp"
            android:id="@+id/tv_num2"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="24dp"
            android:layout_toRightOf="@id/tv_num2"
            android:layout_below="@id/tv_all_name2"
            android:layout_marginTop="18dp"
            android:textSize="12sp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:layout_marginLeft="12dp"
            android:id="@+id/tv_all_num2"/>
        <TextView
            android:layout_width="68dp"
            android:layout_height="24dp"
            android:text="工作状态："
            android:textSize="12sp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:layout_below="@id/tv_num2"
            android:layout_marginTop="18dp"
            android:id="@+id/tv_status2"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="24dp"
            android:layout_toRightOf="@id/tv_num2"
            android:layout_below="@id/tv_all_num2"
            android:layout_marginTop="18dp"
            android:textSize="12sp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:layout_marginLeft="12dp"
            android:id="@+id/tv_all_status2"/>

    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:background="#262728"
        android:layout_below="@id/retlative_sb2"
        android:text="选择任务"
        android:textColor="#ffffff"
        android:textSize="14sp"
        android:gravity="center|left"
        />
    <TextView
        android:layout_width="68dp"
        android:layout_height="24dp"
        android:text="选择任务："
        android:textSize="12sp"
        android:gravity="center"
        android:textColor="#ffffff"
        android:layout_below="@id/retlative_sb2"
        android:layout_marginTop="46dp"
        android:id="@+id/tv_task"/>
    <TextView
        android:layout_width="100dp"
        android:layout_height="26dp"
        android:text="无"
        android:textColor="#ffffff"
        android:textSize="12sp"
        android:layout_below="@id/retlative_sb2"
        android:layout_marginTop="46dp"
        android:layout_marginLeft="68dp"
        android:id="@+id/tv_now_task"
        android:paddingLeft="20dp"
        android:gravity="center|left"
        android:background="@drawable/bt_bg__map"
        />

</RelativeLayout>
