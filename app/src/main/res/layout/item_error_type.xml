<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="753dp"
    android:layout_marginTop="8dp"
    android:orientation="vertical"
    android:layout_height="36dp"
    android:background="#161718">
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:id="@+id/tv_error_id"
        />
    <TextView
        android:layout_width="134dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:layout_marginLeft="36dp"
        android:layout_toRightOf="@id/tv_error_id"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:id="@+id/tv_error_time"
        />
    <TextView
        android:layout_width="134dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:layout_marginLeft="36dp"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/tv_error_time"
        android:id="@+id/tv_error_type"
        />
</RelativeLayout>