<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="753dp"
    android:layout_marginTop="8dp"
    android:orientation="vertical"
    android:layout_height="36dp"
    android:background="#161718">
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:id="@+id/tv_back_id"
        />
    <TextView
        android:layout_width="160dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:layout_marginLeft="26dp"
        android:layout_toRightOf="@id/tv_back_id"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:id="@+id/tv_back_time"
        />
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:layout_marginLeft="37.5dp"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/tv_back_time"
        android:id="@+id/tv_back_pay_f"
        />
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:layout_marginLeft="24dp"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/tv_back_pay_f"
        android:id="@+id/tv_back_back_f"
        />
    <TextView
        android:layout_width="97.5dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:layout_marginLeft="24dp"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/tv_back_back_f"
        android:id="@+id/tv_back_pay_num"
        />
    <TextView
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:textSize="12sp"
        android:textColor="#ffffff"
        android:layout_marginLeft="24dp"
        android:gravity="center"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/tv_back_pay_num"
        android:id="@+id/tv_back_result"
        />

</RelativeLayout>